<!--
	Nonfunctional hosts in *.operavision.eu:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Operavision.eu">
	<target host="operavision.eu" />
	<target host="www.operavision.eu" />

	<rule from="^http:" to="https:" />
</ruleset>
