<!--
	Nonfunctional hosts in *.gettoby.com:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Gettoby.com">
	<target host="gettoby.com" />
	<target host="www.gettoby.com" />

	<rule from="^http:" to="https:" />
</ruleset>
